<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="123.png" />
    <None Remove="add_image-512.webp" />
    <None Remove="new-file-icon.png" />
    <None Remove="Resources\boldTextIcon.png" />
    <None Remove="Resources\italicTextIcon.png" />
    <None Remove="Resources\underlinedTextIcon.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Resources\boldTextIcon.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Resources\italicTextIcon.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Resources\underlinedTextIcon.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <Resource Include="123.png" />
    <Resource Include="add_image-512.webp" />
    <Resource Include="new-file-icon.png" />
  </ItemGroup>

</Project>
